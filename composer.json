{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "codeat3/blade-eos-icons": "^1.15",
        "dedoc/scramble": "^0.12.16",
        "garygreen/pretty-routes": "^1.0",
        "laravel/framework": "^12.0",
        "laravel/reverb": "^1.0",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "mallardduck/blade-lucide-icons": "^1.23",
        "processton/access-controll": "dev-main",
        "processton/client": "dev-main",
        "processton/company": "dev-main",
        "processton/contact": "dev-main",
        "processton/customer": "dev-main",
        "processton/items": "dev-main",
        "processton/lead": "dev-main",
        "processton/campaign": "dev-main",
        "processton/form": "dev-main",
        "processton/locale": "dev-main",
        "processton/org": "dev-main",
        "processton/user-profile": "dev-main",
        "secondnetwork/blade-tabler-icons": "^3.31",
        "vormkracht10/filament-mails": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/app",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/org",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/locale",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/access-controll",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/user-profile",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/company",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/contact",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/customer",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/items",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/lead",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/campaign",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../packages/processton/form",
            "options": {
                "symlink": true
            }
        
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
